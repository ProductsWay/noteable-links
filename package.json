{
  "name": "notable-links",
  "version": "0.0.1",
  "module": "server/index.js",
  "scripts": {
    "dev": "run-p -l dev:*",
    "dev:server": "bun run --watch server/index.ts",
    "server:inspect": "bun --inspect server/index.ts",
    "dev:web": "cd web && bun run dev",
    "format": "prettier --write .",
    "lint": "run-p lint:*",
    "db:generate": "bunx drizzle-kit generate:sqlite",
    "db:migrate": "bun run drizzle/migrate.ts",
    "db:introspect": "bunx  drizzle-kit introspect:sqlite",
    "db:studio": "bunx drizzle-kit studio",
    "lint:server": "eslint --ext .ts,.js server",
    "lint:web": "eslint --ext .ts,.js web",
    "test": "bun test",
    "typecheck": "run-p typecheck:*",
    "typecheck:server": "tsc --noEmit --project server/tsconfig.server.json",
    "typecheck:web": "tsc --noEmit --project web/tsconfig.json"
  },
  "dependencies": {
    "@elysiajs/cors": "0.8.0",
    "@elysiajs/eden": "0.8.1",
    "@elysiajs/static": "0.8.1",
    "@elysiajs/swagger": "0.8.5",
    "@libsql/client": "0.14.0",
    "drizzle-orm": "0.40.0",
    "elysia": "latest",
    "pino": "8.21.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@tsconfig/svelte": "5.0.4",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "bun-types": "latest",
    "drizzle-kit": "0.30.5",
    "eslint": "8.57.1",
    "eslint-config-productsway": "1.3.4",
    "eslint-plugin-svelte": "2.46.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.2",
    "superjson": "2.2.2",
    "svelte": "4.2.19",
    "svelte-check": "3.8.6",
    "tslib": "2.8.1",
    "typescript": "5.8.2",
    "vite": "5.4.14"
  }
}
